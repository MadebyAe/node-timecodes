{
  "name": "node-timecodes",
  "version": "2.3.1",
  "description": "Time and SMTPE Timecodes conversions",
  "license": "MIT",
  "repository": "Synchronized-TV/node-timecodes",
  "author": {
    "name": "Julien Bouquillon",
    "email": "julien.bouquillon@synchronized.tv",
    "url": "http://github.com/Synchronized-TV"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "scripts": {
    "test": "babel-tape-runner spec/**/*.spec.js | tap-spec",
    "build": "babel -d ./dist ./src"
  },
  "main": "dist/index.js",
  "bin": {
    "timecodeFromSeconds": "dist/fromSeconds-bin.js",
    "timecodeToSeconds": "dist/toSeconds-bin.js"
  },
  "keywords": [
    "node",
    "timecode",
    "smtpe",
    "time",
    "conversion"
  ],
  "testling": {
    "files": "test/*.js",
    "browsers": [
      "ie/6..latest",
      "chrome/22..latest",
      "firefox/16..latest",
      "safari/latest",
      "opera/11.0..latest",
      "iphone/5..latest",
      "ipad/5..latest",
      "android-browser/2..latest"
    ]
  },
  "dependencies": {
    "es6-template-strings": "1.0.0"
  },
  "devDependencies": {
    "babel-tape-runner": "1.1.0",
    "tap-spec": "4.0.2",
    "tape": "4.0.0",
    "babel-eslint": "4.0.5",
    "eslint": "0.24.1"
  }
}
